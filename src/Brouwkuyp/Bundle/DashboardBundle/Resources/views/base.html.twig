<!DOCTYPE html>
<html>
<head>
    <meta charset="UTF-8" />
    <title>{% block title %}Brouwkuyp-control{% endblock %}</title>
    {% stylesheets
        'bundles/brouwkuypdashboard/css/switch/normalize.css'
        'bundles/brouwkuypdashboard/css/switch/scaffold.css'
        'bundles/brouwkuypdashboard/css/switch/ios7-switches.css'
        '../src/Brouwkuyp/Bundle/DashboardBundle/Resources/public/less/bootstrap.less'
    %}
    <link rel="stylesheet" href="{{ asset_url }}" />
    {% endstylesheets %}
    <link rel="icon" type="image/x-icon" href="{{ asset('favicon.ico') }}" />
</head>
<body>
    <div class="container-fluid">
        <div id="header">
            <h1><a href="{{ path('home') }}">dashboard van de Saeck</a></h1>
            <div>
                <nav>{{ knp_menu_render('main_menu', { 'currentClass' : 'active' }) }}</nav>
            </div>
        </div>
        {% block content %}{% endblock %}
    </div>
    {% javascripts
    '../components/jquery/jquery.min.js'
    'bundles/brouwkuypdashboard/js/sockjs.min.js'
    'bundles/brouwkuypdashboard/js/stomp.min.js'
    'bundles/brouwkuypdashboard/js/highstock.js'
    'bundles/brouwkuypdashboard/js/bootstrap.js'
    'bundles/brouwkuypdashboard/js/init.js' %}
    <script type="text/javascript" src="{{ asset_url }}"></script>
    {% endjavascripts %}
    {#'bundles/brouwkuypdashboard/js/bootbox.min.js'#}
</body>
</html>
